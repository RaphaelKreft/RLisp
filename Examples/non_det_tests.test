(define prime-sum-pair (lambda (list1 list2)
        (let ((a (eval(list amb list1)))
               (b (eval(list amb list2))))
            (cond ((prime? (+ a b)) (list a b))
                  (#t (amb))))))


(define prime?(lambda (n)
    (eq? n (smallest-divisor n))))

(define smallest-divisor (lambda (n) (find-divisor n 2)))

(define find-divisor (lambda (n test-divisor)
    (cond ((> (square test-divisor) n) n)
          ((divides? test-divisor n) test-divisor)
          (#t (find-divisor n (+ test-divisor 1))))))

(define divides? (lambda (a b) (eq? (% b a) 0)))

(define an-element-of (lambda (items)
    (cond ((nil? items) (amb))
          (#t (amb (car items) (an-elements-of (cdr items)))))))


(println [-->sum-prime-pair (prime-sum-pair (list 1 3 5 8) (list 20 35 110))<--])
(prime-sum-pair (list 1 3 5 8) (list 20 35 110))